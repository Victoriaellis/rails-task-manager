<h1><%= @task.title %></h1>
<p>Details:<%= @task.details %></p>
<p>Complete:<%= @task.completed %></p>

<%= link_to "Back to list", tasks_path %>
<%= link_to "Update this task", edit_task_path %>
<%= link_to "Delete this task", task_path(@task), method: :delete, data: {confirm: "are you sure you want to delete?"} %>
